#include <stdio.h>
#include <stdlib.h>
#define maximo 80
int main()
{
    int vetor [maximo], i;
    float media, soma, notas;
    vetor [maximo] = notas ;
    soma = 0;
    for (i = 0; 1 < 80; i ++) {
        printf("Insira a nota\n");
        scanf ("%d", &notas);
        soma = soma + notas;
    }
    media = soma / 80;
    for (i = 0; 1 < 80; i ++){
        printf("%d e %d\n", notas, media);
    }

}
